{
  "name": "writersblock",
  "version": "0.1.0",
  "description": "Daily Journal and Calendar on the Block Chain",
  "main": "index.js",
  "scripts": {
    "build": "parcel build index.html --out-dir build",
    "cy:o": "cypress open",
    "cy:rec": "cypress run --record --key 1245d772-ce15-434e-b77d-c07769af8bc4",
    "cy:run": "cypress run",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty .",
    "lint-cln": "rimraf .eslintcache && yarn lint",
    "lint-fix": "yarn --silent lint --cache --fix; exit 0",
    "manifest-sync": "chmod 777 manifest.json && cp manifest.json dist/manifest.json",
    "postinstall": "yarn flow-typed",
    "start": "parcel index.html --target browser",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "journal",
    "calendar",
    "decentralized",
    "blockstack"
  ],
  "author": "mlnck",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@hot-loader/react-dom": "^16.8.6",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-styletron-display-name": "^1.1.3",
    "cypress": "^3.4.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-erb": "^0.0.2",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.101.0",
    "flow-typed": "^2.5.2",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime-corejs2": "^7.4.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "blockstack": "^19.3.0",
    "cross-env": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.6",
    "react-router-dom": "^5.0.1",
    "styletron-engine-atomic": "^1.3.0",
    "styletron-react": "^5.1.3"
  },
  "browserslist": [
    "defaults"
  ]
}
